(run){
  # disable colorizing of text messages
  PRETTY_PRINT = Off
  EVENTS = 1000
  EVENT_OUTPUT=HepMC_GenEvent[sherpa_zjets]
  FILE_SIZE = 100000000
  RANDOM_SEED=1 2 3 4
}(run)

(beam){
  BEAM_1 = 2212; BEAM_ENERGY_1 = 4000;
  BEAM_2 = 2212; BEAM_ENERGY_2 = 4000;
}(beam)

(processes){
  Process 93 93 -> 11 -11 93{1}
  Order_EW 2;
  CKKW sqr(30/E_CMS)
  Integration_Error 0.1 {5};
  End process;
}(processes)

(selector){
  Mass 11 -11 66 116
}(selector)

(mi){
  MI_HANDLER = None  # None or Amesic
}(mi)
