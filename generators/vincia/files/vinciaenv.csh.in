#!/bin/sh

set PYTHIA8_LIB_DIR @PYTHIA8_ROOT_DIR@/lib
set ROOT_LIB_DIR @ROOTSYS@/lib
set FASTJET_LIB_DIR @FASTJET_ROOT_DIR@/lib
set VINCIA_LIB_DIR @CMAKE_INSTALL_PREFIX@/lib

setnev LD_LIBRARY_PATH    "${PYTHIA8_LIB_DIR}:${ROOT_LIB_DIR}:${FASTJET_LIB_DIR}:${VINCIA_LIB_DIR}:${LD_LIBRARY_PATH}"
setenv DYLD_LIBRARY_PATH  "${PYTHIA8_LIB_DIR}:${ROOT_LIB_DIR}:${FASTJET_LIB_DIR}:${VINCIA_LIB_DIR}:${DYLD_LIBRARY_PATH}"

setenv PYTHIA8DATA "${PYTHIA8_LIB_DIR}/../xmldoc"
setenv VINCIADATA  "${VINCIA_LIB_DIR}/../xmldoc"



setenv 
